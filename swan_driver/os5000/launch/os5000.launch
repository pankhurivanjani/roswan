<?xml version="1.0"?>
<launch>
    <arg name="port" default="/dev/compass"/>
    <arg name="baud" default="19200"/>
    <arg name="rate" default="40"/>
    <arg name="os5000_topic" default="os5000_data"/>
    
    <node pkg="os5000" type="os5000" name="os5000" output="screen">
        <remap from="os5000_data" to="$(arg os5000_topic)"/>
        <param name="port" value="$(arg port)"/>
        <param name="baud" value="$(arg baud)"/>
        <param name="rate" value="$(arg rate)"/>
    </node>
</launch>
